{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    },
    "overrides": [
        {
            "files": ["**/*.ts?(x)"],
            "rules": {
                "jsx-a11y/anchor-is-valid": 0
            }
        }
    ]
}
